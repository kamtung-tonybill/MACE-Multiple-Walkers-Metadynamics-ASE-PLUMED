# Read the COLVAR file
cv1: READ FILE=COLVAR IGNORE_TIME VALUES=cv1
cv2: READ FILE=COLVAR IGNORE_TIME VALUES=cv2
metad: READ FILE=COLVAR IGNORE_TIME VALUES=metad.rbias

# Define weights
weights: REWEIGHT_METAD TEMP=300

# Calculate histograms

hh: HISTOGRAM ARG=cv1,cv2 GRID_MIN=0.1,0.3 GRID_MAX=0.4,5.0 GRID_BIN=255,255 BANDWIDTH=0.01,0.15 LOGWEIGHTS=weights NORMALIZATION=true CLEAR=5000
DUMPGRID GRID=hh FILE=histogram.dat STRIDE=5000
